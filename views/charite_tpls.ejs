								<div class="ui statistics _nodisplay boxS abs bg-white zind">
									<div class="statistic">
										<div class="value">
										  <img src="artiste2.jpg" class="ui avatar image img_icon">
										  {{participants_}}
										</div>
										<div class="label">
										  <%= locals.value.object.charite_tpls.stats_ %>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
