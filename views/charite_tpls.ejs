<template id="lis_tpl">
	<li class="col-md-4">
		<div class="ui card">
			<div class="content">
				<div class="header text-center">{{titre}}</div>
				<img src="{{img}}" alt="{{titre}}" title="{{titre}}" class="img-responsive"/>
			</div>
			<div class="content" style="padding-bottom:0;">
				<h4 class="ui sub header">{{pays}} {{ville}}, {{lieu}} : <span>{{action}}</span></h4>
				<div class="ui small feed">
					<div class="event">
						<div class="content">
							<div class="summary">
								<a target="_blank" href="/user_account/{ajouteur_href}}" title="{{_ajouteur}}">{{ajouteur_}}</a> <%= locals.value.object.charite_tpls.ajouteur_c %> {{jour}},<%= locals.value.object.charite_tpls.a %>{{heure}}
							</div>
						</div>
					</div>
					<div class="event">
						<div class="content">
							<div class="summary">
